# NextAuth core configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-strong-random-secret

# OAuth providers (placeholders only - put real values in .env.local)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_ID=your-github-client-id
GITHUB_SECRET=your-github-client-secret

# Demo credentials for CredentialsProvider
# Set these in .env.local (do not commit real credentials)
DEMO_USER_EMAIL=demo@example.com
DEMO_USER_PASSWORD=changeme

# Backend JWT secret shared with FastAPI
BACKEND_JWT_SECRET=change-me
